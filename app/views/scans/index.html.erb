<% provide(:title, 'Upload Scanned Log') %>
<div align="center">
<div class="well well-sm" style="width: 50%;">

<%= link_to "Process ftp uploads", scanprocess_path, class: "btn btn-sm btn-primary" %>

<h1>Select a log to upload</h1>
<h2>800 pixels high, jpeg low quality (20%)

<%= form_tag({action: :sort}, multipart: true) do %>
  <%= file_field_tag 'Data' %>
  <%= submit_tag 'Upload', class: "btn btn-small btn-primary" %>
<% end %>
</h2>

</div>
</div>